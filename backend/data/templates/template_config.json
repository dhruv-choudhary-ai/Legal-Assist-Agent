{
  "Lease Agreement": {
    "filename": "property/lease_agreement.docx",
    "category": "Property",
    "description": "Comprehensive lease/rent agreement for property rental",
    "keywords": [
      "lease",
      "rent",
      "rental",
      "property",
      "tenant",
      "landlord"
    ],
    "fields": {
      "location": {
        "label": "Place where agreement is made",
        "type": "text",
        "required": true,
        "example": "Mumbai"
      },
      "day": {
        "label": "Day of agreement",
        "type": "number",
        "required": true,
        "example": "15"
      },
      "month": {
        "label": "Month of agreement",
        "type": "text",
        "required": true,
        "example": "January"
      },
      "year": {
        "label": "Year of agreement",
        "type": "number",
        "required": true,
        "example": "2024"
      },
      "lessor_name": {
        "label": "Landlord/Lessor full name",
        "type": "text",
        "required": true,
        "example": "Mr. Rajesh Kumar"
      },
      "lessor_address": {
        "label": "Landlord/Lessor address",
        "type": "text",
        "required": true,
        "example": "123 MG Road, Mumbai 400001"
      },
      "lessee_name": {
        "label": "Tenant/Lessee full name",
        "type": "text",
        "required": true,
        "example": "Ms. Priya Sharma"
      },
      "lessee_address": {
        "label": "Tenant/Lessee address",
        "type": "text",
        "required": true,
        "example": "456 Park Street, Delhi 110001"
      },
      "property_address": {
        "label": "Complete property address",
        "type": "text",
        "required": true,
        "example": "Flat 302, Green Tower, Bandra West, Mumbai 400050"
      },
      "property_description": {
        "label": "Detailed property description",
        "type": "textarea",
        "required": false,
        "example": "2BHK apartment, 1200 sq ft, with parking"
      },
      "monthly_rent": {
        "label": "Monthly rent amount (₹)",
        "type": "number",
        "required": true,
        "example": "50000"
      },
      "lease_duration_years": {
        "label": "Lease duration in years",
        "type": "number",
        "required": true,
        "example": "1"
      },
      "lease_start_date": {
        "label": "Lease start date",
        "type": "date",
        "required": true,
        "example": "2024-01-01"
      },
      "payment_day": {
        "label": "Rent payment day of month",
        "type": "number",
        "required": true,
        "example": "5"
      },
      "first_payment_date": {
        "label": "First rent payment date",
        "type": "date",
        "required": true,
        "example": "2024-01-05"
      },
      "late_fee_rate": {
        "label": "Late fee interest rate (%)",
        "type": "number",
        "required": false,
        "example": "18"
      },
      "arrears_months": {
        "label": "Months of arrears before termination",
        "type": "number",
        "required": false,
        "example": "2"
      },
      "notice_period": {
        "label": "Notice period for termination (months)",
        "type": "number",
        "required": false,
        "example": "2"
      },
      "lessor_witness_1": {
        "label": "Lessor Witness 1 name",
        "type": "text",
        "required": false,
        "example": "Mr. Amit Patel"
      },
      "lessor_witness_2": {
        "label": "Lessor Witness 2 name",
        "type": "text",
        "required": false,
        "example": "Mr. Suresh Reddy"
      },
      "lessee_witness_1": {
        "label": "Lessee Witness 1 name",
        "type": "text",
        "required": false,
        "example": "Ms. Neha Gupta"
      },
      "lessee_witness_2": {
        "label": "Lessee Witness 2 name",
        "type": "text",
        "required": false,
        "example": "Mr. Vikram Singh"
      }
    }
  },
